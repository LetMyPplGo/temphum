# crontab -e

# Read DHT22 every 10 minutes using venv1 Python
*/10 * * * * cd /etc/temphum && /etc/temphum/venv1/bin/python /etc/temphum/read_data.py >> /etc/temphum/read_data.log 2>&1

# Refresh charts every 10 minutes, offset by 1 minute
1,11,21,31,41,51 * * * * /etc/temphum/make_chart.sh >> /etc/temphum/make_chart.log 2>&1